<mujoco>
	<compiler autolimits="true" eulerseq="XYZ"/>

	<worldbody>
		
        <light diffuse=".5 .5 .5" pos="0 0 3" dir="0 0 -2"/>
		<geom type="plane" size="2 2 0.1" rgba=".9 0 0 1"/>
